<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90"
    android:toDegrees="90"
    android:pivotX="50%"
    android:pivotY="50%" >
    <shape
        android:shape="line">
        <stroke
            android:width="1dp"
            android:color="#CABBBBBB"
            android:dashWidth="0dp"
            android:dashGap="0dp" />
    </shape>
</rotate>